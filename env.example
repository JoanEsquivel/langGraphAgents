# LangSmith Configuration (Optional)
# Copy this file to .env and fill in your values

# Enable LangSmith tracing (use the variables provided by LangSmith)
LANGSMITH_TRACING=true

# Your LangSmith API key (get from https://smith.langchain.com/)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# Optional: Project name for organizing traces
LANGSMITH_PROJECT=langraph-chatbot

# Optional: LangSmith endpoint
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
